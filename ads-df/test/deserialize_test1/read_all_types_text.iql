/* Test that we can read text files with headers.
 */
a = read_block[file="simple_binary.bin"];
s = deserialize[format="a INTEGER, b BIGINT, c DECIMAL, e VARCHAR, f CHAR(8), g DATETIME, h DATE, i TINYINT, j SMALLINT, k IPV4, l IPV6, m CIDRv4, n CIDRv6"];
a -> s;
d = write[file="output.txt", mode="text"];
s -> d;
